#!/bin/bash

# source me...

export PYTHON=/m/vinci9/data1/saunders/the-world/nosync/pythons/python2.7.1-active

export JAVA_HOME="/m/vinci9/data1/saunders/the-world/nosync/javas/jdk1.7.0_07"
export JDK=$JAVA_HOME

pathmunge $JAVA_HOME/bin           before
pathmunge $PYTHON/bin              before

export LD_LIBRARY_PATH="$PYTHON/lib:$JAVA_HOME/lib:$LD_LIBRARY_PATH"
